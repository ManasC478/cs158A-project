<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>

<VBox fx:id="roomButtonGroup" alignment="CENTER" prefHeight="400.0" prefWidth="640.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.stream.controller.HomeController">
    <children>
        <Label text="English" />
        <Button fx:id="createRoom" mnemonicParsing="false" onAction="#createRoom" text="Create Room" />
        <Label fx:id="createRoomWarning" text="Error creating room" visible="false" style="-fx-text-fill: red;"  />
    </children>
</VBox>
